{
  "name": "babel-preset-saya",
  "version": "5.0.0",
  "description": "An opinionated `babel` preset",
  "license": "MIT",
  "author": "Simon Degrave <simon.degraeve@gmail.com>",
  "repository": "github:SimonDegraeve/babel-preset-saya",
  "homepage": "https://github.com/SimonDegraeve/babel-preset-saya#readme",
  "bugs": "https://github.com/SimonDegraeve/babel-preset-saya/issues",
  "main": "index.js",
  "files": [
    "lib",
    "index.js",
    "react.js"
  ],
  "keywords": [
    "babel",
    "babel-preset",
    "saya"
  ],
  "scripts": {
    "lint": "eslint src",
    "test": "jest --config .jestrc --coverage",
    "clean": "rimraf lib",
    "transpile": "babel --copy-files --out-dir lib --ignore *.test.js,__*__ src",
    "build": "npm run clean && npm run transpile",
    "report-coverage": "codecov",
    "release": "terbit --changelog-preset saya"
  },
  "dependencies": {
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-syntax-flow": "^6.13.0",
    "babel-plugin-transform-class-properties": "^6.16.0",
    "babel-plugin-transform-es3-member-expression-literals": "^6.8.0",
    "babel-plugin-transform-es3-property-literals": "^6.8.0",
    "babel-plugin-transform-flow-strip-types": "^6.14.0",
    "babel-plugin-transform-object-rest-spread": "^6.16.0",
    "babel-plugin-transform-regenerator": "^6.16.1",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "fast-async": "github:simondegraeve/fast-async#refactoring"
  },
  "devDependencies": {
    "babel-cli": "^6.16.0",
    "babel-core": "^6.17.0",
    "babel-jest": "^16.0.0",
    "babel-preset-latest": "^6.16.0",
    "codecov": "^1.0.1",
    "conventional-changelog-saya": "*",
    "eslint": "^3.7.1",
    "eslint-config-saya": "*",
    "jest": "^16.0.1",
    "rimraf": "^2.5.4",
    "terbit": "*"
  }
}
